{
  "name": "todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "server": "env-cmd -f .env nodemon --inspect server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "engines": {
    "node": "12.13.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "core-js": "^3.3.2",
    "express": "^4.17.1",
    "mongodb": "^3.4.0",
    "mongoose": "^5.8.1",
    "nodemon": "^1.19.4",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.5",
    "vue-material": "^1.0.0-beta-11",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "env-cmd": "10.0.1",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "~5.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}